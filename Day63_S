class Solution 
{
    
public long countSubarrays(int[] nums, int minK, int maxK) 
{
    
long ans =0;
    
int j=-1;
    
int preminKIndex =-1;
    
int premaxKIndex=-1;

for(int i=0; i<nums.length; i++)
{
    if(nums[i]<minK || nums[i]>maxK)      
        j=i;
    
      if (nums[i] == minK)
    preminKIndex = i;
    
  if (nums[i] == maxK)
    premaxKIndex = i;
  
  ans += Math.max(0, Math.min(preminKIndex, premaxKIndex) - j);
    
} 
return ans;   
}
}
